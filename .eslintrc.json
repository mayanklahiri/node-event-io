{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false,
      "impliedStrict": true
    }
  },
  "extends": "airbnb-base",
  "plugins": [],
  "rules": {
    "one-var": 0,
    "no-unused-vars": 1,
    "no-console": 0,
    "prefer-spread": 0
  }
}
