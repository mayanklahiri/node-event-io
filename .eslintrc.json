{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false,
      "impliedStrict": true
    }
  },
  "extends": "airbnb-base",
  "plugins": [],
  "rules": {
    "one-var": 0,
    "no-unused-vars": 1,
    "no-console": 0,
    "prefer-spread": 0,
    "no-underscore-dangle": 0,
    "no-multi-assign": 0,
    "no-lonely-if": 0,
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": 1
  }
}
